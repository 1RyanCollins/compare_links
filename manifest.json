{
  "manifest_version": 3,
  "name": "SERP vs ChatGPT Links",
  "version": "1.0",
  "description": "Compare Google SERP links vs ChatGPT response links",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "js": ["google.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["chatgpt.js"]
    }
  ]
}
